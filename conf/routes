# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET         /                                           controllers.Application.index
POST        /api/deploy/:stackName                      controllers.Application.deploy(stackName:String)
GET         /api/deploy/status-events/:stackName        controllers.Application.statusSocket(stackName:String)

GET         /system-info/version                        controllers.Application.buildInfo
GET         /system-info/healthcheck                    controllers.Application.healthCheck
